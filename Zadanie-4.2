import java.io.File;
import java.io.FileNotFoundException;
import java.util.*;

public class Zadanie42 {


    public static void main(String[] args) throws FileNotFoundException {
        Scanner scanner = new Scanner(new File("C:\\Users\\erixv\\Documents\\Inf\\Okregi.txt"));
        int X =0;
        List<String> Okr= new ArrayList<>();
        while(scanner.hasNextLine()) {
            Okr.add(scanner.nextLine());
        }
        List<String> S = new ArrayList<>();
        for (int i = 0; i < Okr.size(); i++) {
            String[] a =Okr.get(i).split(" ");

            if(Math.abs(Integer.valueOf(a[1]))==Math.abs(Integer.valueOf(a[2]))) {
                X++;
                S.add(Okr.get(i));
            }
        }
        Collections.sort(S);
        for (int i = 0; i < S.size(); i++) {
            System.out.println(S.get(i));
        }
        System.out.println(X);

    }

}
